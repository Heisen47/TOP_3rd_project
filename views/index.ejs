<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p> <% messages.forEach(message => { %>
          <div>
            <strong> <%= message.user %> </strong>
            <%= message.text %>
            <span>( Added: <%= message.added %>)</span>
          </div>
        <%  }); %>
    </p>
    <a href="http://localhost:3000/new">Go to Form</a>
  </body>
</html>
